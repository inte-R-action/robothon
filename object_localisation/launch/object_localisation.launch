<launch>

  <!-- By default, we are not in debug mode -->
  <arg name="test_mode" default="false" />
  <arg name="task" default="assemble_complex_box" />
  <arg name="classifier" default="one" />

  <node name="run_object_localisation" pkg="object_localisation" type="run_object_localisation.py" respawn="false" output="screen">
  </node>

  <node name="balloon_test" pkg="object_localisation" type="balloon_test.py" respawn="false" output="screen">
  </node>

  <node pkg="tf" type="static_transform_publisher" name="baselink_pub" args="0 0 0 0 0 0 1 world base_link 100" />
  <node pkg="tf" type="static_transform_publisher" name="camera_frame_broadcaster" args="0.065 0.037 0.082 0 0 0 1 base_link camera_frame 100" />


</launch>
